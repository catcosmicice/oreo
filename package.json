{
	"name": "oreo",
	"version": "1.0.0",
	"description": "A multipurpose discord bot.",
	"main": "dist/src/index.js",
	"repository": "https://github.com/oweo/oreo",
	"author": "catcosmicice <catcosmicice@protonmail.ch>",
	"license": "CC BY-NC-ND 4.0",
	"prettier": {
		"tabWidth": 4,
		"singleQuote": false,
		"semi": true,
		"trailingComma": "none",
		"arrowParens": "always",
		"printWidth": 80
	},
	"scripts": {
		"dev": "yarn compile && yarn rd",
		"compile": "rm -rf dist/ && yarn tsc",
		"rd": "NODE_ENV=development yarn node dist/src/index.js",
		"prettify": "yarn prettier . --write"
	},
	"engines": {
		"node": ">=14.0.0."
	},
	"_moduleAliases": {
		"@oreo/src": "dist/src",
		"@oreo/lib": "dist/lib",
		"@oreo/config": "dist/config"
	},
	"devDependencies": {
		"@types/module-alias": "^2",
		"@types/node": "^14.14.37",
		"@types/prettier": "^2",
		"@yarnpkg/pnpify": "^2.4.0",
		"prettier": "^2.2.1",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"@aero/centra": "^1.0.6",
		"discord-akairo": "^8.1.0",
		"discord.js": "^12.5.3",
		"dotenv": "^8.2.0",
		"module-alias": "^2.2.2"
	}
}
